Jonathan :
function aVeryBigSum(n, ar) {
let sum = 0;
  for (let i = 0; i < n; i++) {
    sum = sum + ar[i];
  }
  return sum;
}
console.log(aVeryBigSum(5, [1000000001,1000000002,1000000003,1000000004,1000000005])) 

